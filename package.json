{
  "name": "utility-kit",
  "version": "0.10.0",
  "description": "An easy to use library that provides miscellaneous but very useful functions that are commonly used in most javascript projects.",
  "license": "MIT",
  "author": "Sahil Aggarwal <aggarwalsahil2004@gmail.com>",
  "homepage": "https://github.com/SahilAggarwal2004/utility-kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SahilAggarwal2004/utility-kit.git"
  },
  "bugs": {
    "url": "https://github.com/SahilAggarwal2004/utility-kit/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./cache": {
      "import": "./dist/esm/lib/cache.js",
      "require": "./dist/cjs/lib/cache.cjs"
    },
    "./math": {
      "import": "./dist/esm/lib/math.js",
      "require": "./dist/cjs/lib/math.cjs"
    },
    "./random": {
      "import": "./dist/esm/lib/random.js",
      "require": "./dist/cjs/lib/random.cjs"
    },
    "./time": {
      "import": "./dist/esm/lib/time.js",
      "require": "./dist/cjs/lib/time.cjs"
    },
    "./types": "./dist/esm/types.js",
    "./utility": {
      "import": "./dist/esm/lib/utility.js",
      "require": "./dist/cjs/lib/utility.cjs"
    }
  },
  "main": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm i && pnpm run compile",
    "compile": "tsup",
    "dev": "tsup --watch",
    "dry-release": "release-it --ci --dry-run",
    "prettier": "prettier-package-json --write package.json",
    "pub": "pnpm login && pnpm publish",
    "release": "release-it --ci"
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.4",
    "@types/node": "^25.0.9",
    "prettier-package-json": "^2.8.0",
    "release-it": "^19.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "helpers",
    "javascript",
    "math",
    "memoize",
    "otp-generator",
    "random",
    "random-name-generator",
    "time",
    "typescript",
    "utility-functions",
    "utility-kit",
    "utils"
  ]
}
