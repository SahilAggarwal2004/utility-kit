{
  "name": "utility-kit",
  "version": "0.8.2",
  "description": "An easy to use library that provides miscellaneous but very useful functions that are commonly used in most javascript projects.",
  "license": "MIT",
  "author": "Sahil Aggarwal",
  "homepage": "https://github.com/SahilAggarwal2004/utility-kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SahilAggarwal2004/utility-kit.git"
  },
  "bugs": {
    "url": "https://github.com/SahilAggarwal2004/utility-kit/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./cache": {
      "import": "./dist/esm/cache.js",
      "require": "./dist/cjs/cache.js"
    },
    "./math": {
      "import": "./dist/esm/math.js",
      "require": "./dist/cjs/math.js"
    },
    "./random": {
      "import": "./dist/esm/random.js",
      "require": "./dist/cjs/random.js"
    },
    "./time": {
      "import": "./dist/esm/time.js",
      "require": "./dist/cjs/time.js"
    },
    "./utility": {
      "import": "./dist/esm/utility.js",
      "require": "./dist/cjs/utility.js"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm i && pnpm run compile",
    "compile": "tsup",
    "prettier": "prettier-package-json --write package.json"
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "prettier-package-json": "^2.8.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "helpers",
    "javascript",
    "math",
    "memoize",
    "otp-generator",
    "random",
    "random-name-generator",
    "time",
    "typescript",
    "utility-functions",
    "utility-kit",
    "utils"
  ]
}
